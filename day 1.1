<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean class="com.stackroute.springCore.Employee" name="a1">
		<property name="ename" value="Mohammad zaid" />
		<property name="age" value="21" />
		<property name="city" value="Banglore" />
		<property name="name">
			<list>
				<value>Deepak</value>
				<value>Rahul</value>
				<value>Aniket</value>

			</list>
		</property>

	</bean>

	<bean class="com.stackroute.springCore.Student" name="std1">
		<property name="sname" value="Deepak" />
		<property name="marks" value="95" />


	</bean>


</beans>
